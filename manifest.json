{
	"manifest_version": 2,
	"name": "MO Url Capturer",
	"version": "1.0",
	"description": "Captura las URLs que han sido visitadas.",
	"background": {
		"scripts": ["/js/background.js"],
		"persistent": false
	},
	"permissions": ["storage","history"],
	"browser_action": {
		"default_title": "Multimodal Observer",
		"default_popup": "/html/popup.html",
		"default_icon": {
        "16": "/images/logos/mo_logo@16x16.png",
        "32": "/images/logos/mo_logo@32x32.png",
        "48": "/images/logos/mo_logo@48x48.png",
        "128": "/images/logos/mo_logo@128x128.png"
      }
	}
}