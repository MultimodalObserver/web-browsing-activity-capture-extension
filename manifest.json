{
	"manifest_version": 2,
	"name": "__MSG_extensionName__",
	"version": "1.0",
	"description": "__MSG_extensionDescription__",
	"default_locale": "en",
	"content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["/js/content/globalVariables.js","/js/content/eventHandlers.js","/js/util/throttle.js","/js/content/content.js"]
        }
    ],
	"background": {
		"scripts": ["/js/util/serverError.js","/js/background/globalVariables.js","/js/util/ajax.js","/js/background/messageActionsMap.js", "/js/background/background.js"]
	},
	"permissions": ["storage", "tabs"],
	"browser_action": {
		"default_title": "__MSG_extensionDefaultTitle__",
		"default_popup": "/html/popup.html",
		"default_icon": {
        "16": "/images/logos/mo_logo@16x16.png",
        "32": "/images/logos/mo_logo@32x32.png",
        "48": "/images/logos/mo_logo@48x48.png",
        "128": "/images/logos/mo_logo@128x128.png"
      }
	}
}